{
	/* Settings */
	"passfail": false,
	/* Environment */
	"node": true,
	"browser": true,
	"phantom": true,
	"jasmine": true,
	"jquery": true,
	"es3": false,
	"strict": true,
	/* Code Style */
	"esnext": true,
	"bitwise": true,
	"curly": true,
	"eqeqeq": true,
	"immed": true,
	"latedef": true,
	"newcap": false,
	"noarg": true,
	"quotmark": "single",
	"regexp": true,
	"smarttabs": true,
	"sub": true,
	"trailing": true,
	"undef": false,
	"unused": true,
	/* Global variables */
	"globals": {
		"angular": false,
		/* AMD */
		"define": false,
		"require": false,
		"requirejs": false,
		/* Custom */
		"ngRoutes": false,
		"ngSanitize": false,
		"ngCookies": false,
		"ngResource": false,
		"appConfig": false,
		"module": true,
		"_": false,
		"$": false,
		"t": false,
		"supplant": false,
		/* Tests */
		"after": false,
		"afterEach": false,
		"before": false,
		"beforeEach": false,
		"browser": false,
		"describe": false,
		"ddescribe": false,
		"dump": false,
		"expect": false,
		"inject": false,
		"it": false,
		"iit": false,
		"jasmine": false,
		"spyOn": false,
		"xit": false
	}
}
